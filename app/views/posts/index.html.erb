<h1>SenseOfCommitment</h1>
<h3>Tweet一覧</h3>
<%= link_to "新規投稿へ", posts_new_path %>
<div class="post-container">
  <% @post.each do |t| %>
    <div class="post">
    <%= t.user.email %>
      <%= t.title %>
      <%= t.about %>
      <%= t.user_id %>
      <%= link_to "詳細へ", post_path(t.id) %>

    </div>
  <% end %>
</div>